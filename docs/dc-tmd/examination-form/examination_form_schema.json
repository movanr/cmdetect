          "examiner": { "type": "boolean", "label": "Examiner" },
          "patient": { "type": "boolean", "label": "Patient" }
        },
        "crepitus": {
          "examiner": { "type": "boolean", "label": "Examiner" },
          "patient": { "type": "boolean", "label": "Patient" }
        },
        "pain_with_click": { "type": "boolean", "label": "Pain w/ Click" },
        "familiar_pain": { "type": "boolean", "label": "Familiar Pain" }
      },
      "left": {
        "click": {
          "examiner": { "type": "boolean", "label": "Examiner" },
          "patient": { "type": "boolean", "label": "Patient" }
        },
        "crepitus": {
          "examiner": { "type": "boolean", "label": "Examiner" },
          "patient": { "type": "boolean", "label": "Patient" }
        },
        "pain_with_click": { "type": "boolean", "label": "Pain w/ Click" },
        "familiar_pain": { "type": "boolean", "label": "Familiar Pain" }
      }
    },

